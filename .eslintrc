{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "rules": {
    "no-plusplus": "off",
    "no-multi-spaces": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off"
  }
}